



def display_menu ():
    print("""
Contact Book Menu:
1. Add Contact
2. View Contact
3. Edit Contact
4. Delete Contact
5. List All Contacts
6. Exit
""")
def add_contact(contact_book):
    name = input()
    email = input()
    address = input()
    phone = input()
    if name in contact_book.keys() : 
      print("Contact already exists!")
      return
    else : 
      contact_book[name] = {"phone":phone,"email":email,"address":address}
      print("Contact added successfully!")
      return

